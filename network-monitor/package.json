  "bugs": {
    "url": "https://github.com/tmigone/tskelly/issues"
  },
  "homepage": "https://github.com/tmigone/tskelly#readme",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/speedtest-net": "^2.1.1",
    "lint-staged": "^11.2.0",
    "nodemon": "^2.0.13",
    "rimraf": "^3.0.2",
    "ts-node": "^10.2.1",
    "ts-standard": "^10.0.0",
    "typescript": "^4.4.3"
  },
  "files": [
    "bin",
    "build"
  ],
  "dependencies": {
    "axios": "^0.23.0",
    "express": "^4.17.1",
    "express-ping": "^1.4.0",
